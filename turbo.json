{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "dist/**",
        "storybook-static/**"
      ]
    },
    "test": {
      "dependsOn": [
        "^build"
      ]
    },
    "lint": {},
    "typecheck": {},
    "dev": {},
    "e2e": {
      "dependsOn": [
        "build"
      ]
    },
    "gen": {}
  }
}
